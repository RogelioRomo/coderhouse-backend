<main>
    {{!< layouts/main}}
    <section>
        <h1>Users</h1>
        {{#each users}}
            <p><strong>Name: </strong>{{this.first_name}} {{this.last_name}}- <strong>Email: </strong>{{this.email}}</p>
        {{/each}}
        {{#if hasPrevPage}}
            <a href="/users?pageQuery={{prevPage}}">Prev</a>
        {{/if}}
        <label>{{page}}</label>
        {{#if hasNextPage}}
            <a href="/users?pageQuery={{nextPage}}">Next</a>
        {{/if}}
    </section>
</main>